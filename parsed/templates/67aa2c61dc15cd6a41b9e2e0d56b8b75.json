{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\r\n<ul class=\"navUser-section sf-menu\">\r\n    <li class=\"navUser-item\">\r\n        <span class=\"navUser-action navUser-action--currencySelector has-dropdown\"\r\n           href=\"#\"\r\n        >\r\n                <span class=\"flag\">{{{lang 'common.currency-flag' flag=currency_selector.active_currency_flag}}}</span><span class=\"code\">{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-currency-arrow\" />\r\n                    </svg>\r\n                </i>\r\n            </span>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            {{#if is_active \"!=\" true}}\r\n              <li>\r\n                  <a href=\"{{{switch_url}}}\">\r\n                    <span class=\"flag\">{{{flag}}}</span><span class=\"code\">{{code}}</span>\r\n                  </a>\r\n              </li>\r\n            {{/if}}\r\n          {{/each}}\r\n        </ul>\r\n    </li>\r\n</ul>\r\n{{/if}}\r\n\r\n\r\n"}
