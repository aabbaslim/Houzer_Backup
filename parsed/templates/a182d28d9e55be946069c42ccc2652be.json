{"components/1center-custom/oc-navigation-mobile":"<nav class=\"navUser mobile-navigation-link\">\r\n    \r\n    <ul class=\"navUser-section navUser-section--alt sf-menu\">\r\n       \r\n        {{#if theme_settings.enable-product-compare}}\r\n        <li class=\"navUser-item compare-header\">\r\n            <a class=\"navUser-action navUser-item--compare-custom\"\r\n               href=\"{{urls.compare}}\"\r\n               data-compare-nav\r\n               aria-label=\"{{lang 'common.compare'}}\"\r\n            >\r\n            <svg>\r\n                <use xlink:href=\"#icon-header-compare\" />\r\n            </svg>\r\n            <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n            </a>\r\n        </li>\r\n        {{/if}}\r\n        \r\n        <li class=\"navUser-item navUser-item--account\">\r\n            <a class=\"navUser-action\" href=\"javascript:void(0)\" aria-label=\"{{lang 'common.account'}}\">\r\n                <svg>\r\n                    <use xlink:href=\"#icon-header-user\" />\r\n                </svg>\r\n            </a>\r\n            <ul>\r\n                <li class=\"navUser-item\">\r\n                    {{#if customer}}\r\n                    <a class=\"navUser-action\" href=\"{{urls.auth.logout}}\" aria-label=\"{{lang 'common.logout'}}\">\r\n                        {{lang 'common.logout'}}\r\n                    </a>\r\n                    {{else}}\r\n                    <a class=\"navUser-action\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'common.login'}}\">\r\n                        {{lang 'common.login'}}\r\n                    </a>\r\n                    {{/if}}\r\n                </li>\r\n                <li class=\"navUser-item\">\r\n                    {{#if customer}}\r\n                        <a class=\"navUser-action\" href=\"{{urls.account.index}}\" aria-label=\"{{lang 'common.account'}}\">\r\n                            {{lang 'common.account'}}\r\n                        </a>\r\n                    {{else}}\r\n                        {{#if settings.account_creation_enabled}}\r\n                            <a class=\"navUser-action\" href=\"{{urls.auth.create_account}}\" aria-label=\"{{lang 'common.sign_up'}}\">\r\n                                {{lang 'common.sign_up'}}\r\n                            </a>\r\n                        {{/if}}\r\n                    {{/if}}\r\n                </li>\r\n                {{#if settings.gift_certificates_enabled}}\r\n                <li class=\"navUser-item\">\r\n                    <a class=\"navUser-action\" href=\"{{urls.gift_certificate.purchase}}\"\r\n                        aria-label=\"{{lang 'common.gift_cert'}}\">\r\n                        {{lang 'common.gift_cert'}}\r\n                    </a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if customer}}\r\n                <li class=\"navUser-item\">\r\n                    <a class=\"navUser-action\" href=\"{{urls.account.wishlists.all}}\"\r\n                        aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\">\r\n                        {{lang 'account.mobile_nav.wishlists'}}\r\n                    </a>\r\n                </li>\r\n                {{else}}\r\n                <li class=\"navUser-item\">\r\n                    <a class=\"navUser-action\" href=\"{{urls.auth.login}}\"\r\n                        aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\">\r\n                        {{lang 'account.mobile_nav.wishlists'}}\r\n                    </a>\r\n                </li>\r\n                {{/if}}\r\n                {{#if customer.store_credit.value '>' 0}}\r\n                    <li class=\"navUser-item\">\r\n                        <a class=\"navUser-action navUser-action--storeCredit\"\r\n                        data-dropdown=\"storeCredit-dropdown\"\r\n                        data-options=\"align:bottom\"\r\n                        href=\"{{urls.cart}}\"\r\n                        aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\r\n                        >\r\n                            \r\n                                {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                            \r\n                        </a>\r\n                        <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                            {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n                        </div>\r\n                    </li>\r\n                {{/if}}\r\n            </ul>\r\n        </li>\r\n        <li class=\"navUser-item navUser-item--cart\">\r\n            <a class=\"navUser-action\" data-cart-preview data-dropdown=\"cart-preview-dropdown\" data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\" aria-label=\"{{lang 'cart.nav_aria_label'}}\" title=\"Cart\">\r\n                <span class=\"navUser-item-cartLabel\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-header-basket\" />\r\n                    </svg>\r\n                </span>\r\n                <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n    </ul>\r\n</nav>\r\n"}
