{"components/1center-custom/oc-pagenavigation-list":"{{#if children}}\r\n<a class=\"navPages-action has-subMenu{{#if name '==' title}} activePage{{/if}}\" href=\"{{url}}\">\r\n    <span>{{name}}</span>\r\n    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n    </i>\r\n</a>\r\n\r\n<ul class=\"navPage-subMenu-simplelist custom-scroll\">\r\n    <div class=\"simple-megamenu-inner\">\r\n        \r\n            {{#each children}}\r\n            <li class=\"navPages-item navPages-item-page\">\r\n                <!-- THIRD LEVEL STARTS -->\r\n                {{#if children}}\r\n                <a href=\"{{url}}\" class=\"needsclick navPages-action has-subMenu{{#if name '==' title}} activePage{{/if}}\">\r\n                    {{name}}\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                    </i>\r\n                </a>\r\n                <ul class=\"nav-submenu nav-third-level\">\r\n                    {{#each children}}\r\n                    <li class=\"navPages-item navPages-item-page\">\r\n        \r\n                      <!-- FOURTH LEVEL STARTS -->\r\n                      {{#if children}}\r\n                      <a href=\"{{url}}\" class=\"needsclick navPages-action has-subMenu{{#if name '==' title}} activePage{{/if}}\">\r\n                          {{name}}\r\n                          <i class=\"icon\" aria-hidden=\"true\">\r\n                            <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                        </i>\r\n                      </a>                    \r\n                      <ul class=\"nav-submenu nav-fourth-level\">\r\n                            {{#each children}}\r\n                            <li class=\"navPages-item navPages-item-page\">\r\n                \r\n                              <!-- FIFTH LEVEL STARTS -->\r\n                              <a href=\"{{url}}\" class=\"needsclick navPages-action has-subMenu{{#if name '==' title}} activePage{{/if}}\">\r\n                                  {{name}}\r\n                              </a>                    \r\n                            </li>\r\n                            {{/each}}\r\n                        </ul>\r\n                        {{else}}\r\n                        <a href=\"{{url}}\" class=\"needsclick navPages-action has-subMenu{{#if name '==' title}} activePage{{/if}}\">{{name}}</a>\r\n                        {{/if}}\r\n                        <!-- FOURTH LEVEL STARTS -->\r\n                    </li>\r\n                    {{/each}}\r\n                </ul>\r\n                {{else}}\r\n                <a href=\"{{url}}\" class=\"needsclick navPages-action has-subMenu{{#if name '==' title}} activePage{{/if}}\">{{name}}</a>\r\n                {{/if}}\r\n                <!-- THIRD LEVEL STARTS -->\r\n            </li>\r\n            {{/each}}\r\n       </div>\r\n    </ul>\r\n\r\n{{else}}\r\n<a class=\"navPages-action  {{#if name '==' title}} activePage{{/if}}\"\r\n   href=\"{{url}}\"\r\n   aria-label=\"{{name}}\"\r\n>\r\n    <span>{{name}}</span>\r\n</a>\r\n{{/if}}\r\n"}
