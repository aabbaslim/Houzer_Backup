           </div>\r\n                {{/if}}\r\n                {{/or}}\r\n                {{/if}}\r\n                {{#or theme_settings.show_powered_by theme_settings.show_copyright_footer}}\r\n               \r\n                    <div class=\"footer-copyright grid\">\r\n                       \r\n                        <p class=\"powered-by\"> {{#if theme_settings.show_copyright_footer}} &copy; {{moment format=\"YYYY\"}} {{settings.store_name}} | All Rights Reserved |{{/if}} <a href=\"https://www.houzersink.com/sitemap\">Sitemap</a> | <a href=\"https://www.houzersink.com/privacy-policy/\">Privacy Policy</a> | <a href=\"https://www.houzersink.com/terms-of-service-for-sms-subscribers/\">Terms of Service for SMS Suscribers</a> | <a href=\"https://www.houzersink.com/terms-and-conditions/\">Terms and Conditions</a> {{#if theme_settings.show_powered_by}} {{lang 'footer.powered_by'}} <a href=\"https://www.1center.co/\" rel=\"nofollow\">1Center</a>{{/if}} </p>\r\n                        \r\n                    </div>\r\n                    {{/or}}\r\n                \r\n                {{> components/common/payment-icons}}\r\n                \r\n                \r\n            </div>\r\n        </div>\r\n    </div>\r\n    {{{region name=\"ssl_site_seal--global\"}}}\r\n</footer>\r\n","components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\r\n<ul class=\"navUser-section sf-menu\">\r\n    <li class=\"navUser-item\">\r\n        <span class=\"navUser-action navUser-action--currencySelector has-dropdown\"\r\n           href=\"#\"\r\n        >\r\n                <span class=\"flag\">{{{lang 'common.currency-flag' flag=currency_selector.active_currency_flag}}}</span><span class=\"code\">{{lang 'common.currency' code=currency_selector.active_currency_code}}</span>\r\n                <i class=\"icon\" aria-hidden=\"true\">\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-currency-arrow\" />\r\n                    </svg>\r\n                </i>\r\n            </span>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            {{#if is_active \"!=\" true}}\r\n              <li>\r\n                  <a href=\"{{{switch_url}}}\">\r\n                    <span class=\"flag\">{{{flag}}}</span><span class=\"code\">{{code}}</span>\r\n                  </a>\r\n              </li>\r\n            {{/if}}\r\n          {{/each}}\r\n        </ul>\r\n    </li>\r\n</ul>\r\n{{/if}}\r\n\r\n\r\n","components/common/payment-icons":"\r\n    {{#with theme_settings}}\r\n        {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay show_accept_klarna}}\r\n        <div class=\"grid  {{#or ../theme_settings.show_powered_by ../theme_settings.show_copyright_footer}} {{else}} full-wdith-payment{{/or}}\">\r\n        <div class=\"footer-payment-icons\">\r\n            {{#if show_accept_amex}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-footer-american-express\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_discover}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_mastercard}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-footer-master-card\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_paypal}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-footer-paypal\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_visa}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-footer-visa\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_amazonpay}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-footer-amzzon-pay\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_googlepay}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\r\n            {{/if}}\r\n            {{#if show_accept_klarna}}\r\n            <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-klarna\"></use></svg>\r\n            {{/if}}\r\n        </div>\r\n    </div>\r\n        {{/or}}\r\n    {{/with}}\r\n\r\n","components/common/social-links":"{{#if social_media}}\r\n    <ul class=\"socialLinks socialLinks--alt\">\r\n        {{#each social_media}}\r\n            <li class=\"socialLinks-item\">\r\n            \r\n                \r\n                <a class=\"icon icon--{{display_name}}\"\r\n                   href=\"{{url}}\"\r\n                   target=\"_blank\"\r\n                   rel=\"noopener\"\r\n                   title=\"{{{capitalize display_name}}}\"\r\n                >\r\n                    <span class=\"aria-description--hidden\">{{{capitalize display_name}}}</span>\r\n                    <svg>\r\n                        <use xlink:href=\"#icon-footer-{{display_name}}\"/>\r\n                    </svg>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n    </ul>\r\n{{/if}}\r\n","components/common/subscription-form":"<h3 class=\"footer-title\">{{lang 'newsletter.subscribe'}}</h3>\r\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\"\r\n                       id=\"nl_email\"\r\n                       name=\"nl_email\"\r\n                       type=\"email\"\r\n                       value=\"{{customer.email}}\"\r\n                       placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\r\n                       aria-describedby=\"alertBox-message-text\"\r\n                       aria-required=\"true\"\r\n                       required\r\n                >\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\"\r\n                       type=\"submit\"\r\n                       value=\"{{lang 'newsletter.subscribe_submit'}}\"\r\n                >\r\n            </div>\r\n           {{!-- {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}} --}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}
