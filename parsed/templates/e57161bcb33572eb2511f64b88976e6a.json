{"components/common/paginator":"<nav class=\"pagination\" aria-label=\"pagination\">\r\n    <ul class=\"pagination-list\">\r\n        {{#each links}}\r\n            {{#if @index '===' 0}}\r\n                <li class=\"pagination-item pagination-item--previous pfirst\">\r\n                    <a class=\"pagination-link\"\r\n                       href=\"{{this.url}}\"\r\n                    >\r\n                        <i class=\"icon\" aria-hidden=\"true\">\r\n                            <svg>\r\n                                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-pagination-previcon\"></use>\r\n                            </svg>\r\n                            <svg>\r\n                                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-pagination-previcon\"></use>\r\n                            </svg>\r\n                        </i>\r\n                        <span class=\"pagination-text\">First</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{previous}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.previous'}}\"\r\n                >\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-pagination-previcon\"></use>\r\n                        </svg>\r\n                    </i>\r\n                    <span class=\"pagination-text\">{{lang 'common.previous'}}</span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each links}}\r\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{this.url}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   {{#if this.number '==' ../current}}\r\n                   aria-current=\"page\"\r\n                   data-pagination-current-page-link\r\n                   {{/if}}\r\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\r\n                >\r\n                    {{this.number}}\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\"\r\n                   href=\"{{next}}\"\r\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\r\n                   aria-label=\"{{lang 'common.next'}}\"\r\n                >\r\n                <span class=\"pagination-text\">{{lang 'common.next'}}</span>\r\n                    <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-pagination-nexticon\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each links}}\r\n            {{#if @last}}\r\n                <li class=\"pagination-item pagination-item--next plast\">\r\n                    <a class=\"pagination-link\"\r\n                       href=\"{{this.url}}\"\r\n                    >\r\n                        <i class=\"icon\" aria-hidden=\"true\">\r\n                            <svg>\r\n                                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-pagination-nexticon\"></use>\r\n                            </svg>\r\n                            <svg>\r\n                                <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-pagination-nexticon\"></use>\r\n                            </svg>\r\n                        </i>\r\n                        <span class=\"pagination-text\">Last</span>\r\n                    </a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n</nav>\r\n"}
