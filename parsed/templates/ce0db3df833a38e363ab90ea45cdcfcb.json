{"components/1center-custom/oc-custom-fields":"{{#each product.custom_fields}}\r\n                    <li class=\"oc-customfield-row grid\">\r\n                        <div class=\"oc-customfield-inner\">\r\n                            <dt class=\"productView-info-name\">{{name}}:</dt>\r\n                            <dd class=\"productView-info-value\">{{{ sanitize value}}}</dd>\r\n                        </div>\r\n                    </li>\r\n                {{/each}}\r\n","components/1center-custom/oc-description":"<div class=\"oc-description-tabs\">\r\n    \r\n    {{assignVar 'checkPrdFeatures' '0'}}\r\n\t{{assignVar 'checkPrdSpec' '0'}}\r\n\t{{assignVar 'checkPrdDl' '0'}}\r\n\t\r\n\t{{#each product.custom_fields}}\r\n\t\t{{#startsWith \"_feat_\" name}}\r\n\t\t\t{{assignVar 'checkPrdFeatures' '1'}}\r\n\t\t{{/startsWith}}\r\n\t{{/each}}\r\n\r\n\t{{#each product.custom_fields}}\r\n\t\t{{#startsWith \"_spec_\" name}}\r\n\t\t\t{{assignVar 'checkPrdSpec' '1'}}\r\n\t\t{{/startsWith}}\r\n\t{{/each}}\r\n\t\r\n\t{{#each product.custom_fields}}\r\n\t\t{{#startsWith \"_dl_\" name}}\r\n\t\t\t{{assignVar 'checkPrdDl' '1'}}\r\n\t\t{{/startsWith}}\r\n\t{{/each}}\r\n\t\r\n\t{{#if (getVar 'checkPrdFeatures') '==' '1'}}\r\n\t\t<div class=\"oc-description-tabrow\">\r\n\t\t\t<div class=\"tab\">\r\n\t\t\t\t<h2 class=\"tab-title\">FEATURES & BENEFITS</h2>\r\n\t\t\t\t<div class=\"icon\">\r\n\t\t\t\t\t<svg>\r\n\t\t\t\t\t\t<use xlink:href=\"#icon-pdp-tab-arrow\" />\r\n\t\t\t\t\t</svg>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"tab-content oc-productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\r\n\t\t\t    <ul>\r\n\t\t\t\t    {{#each product.custom_fields}}\r\n\t\t\t\t\t    {{#startsWith \"_feat_\" name}}\r\n\t\t\t\t\t\t    <li>{{{value}}}</li>\r\n\t\t\t\t\t    {{/startsWith}}\r\n\t\t\t\t    {{/each}}\r\n\t\t\t\t</ul>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t{{/if}}\r\n\t\r\n    {{#if product.description}}\r\n        {{#contains product.description '<p><!-- pagebreak --></p>'}}\r\n            {{#eachIndex (split product.description '<p><!-- pagebreak --></p>')}}\r\n                {{assignVar 'checktabTitle' '0'}}\r\n                \r\n                {{#each (split item '</h4>')}}\r\n                    {{#if @key '==' '0'}}\r\n                        {{#or (if (getVar 'checkPrdFeatures') '==' '1') (if (getVar 'checkPrdSpec') '==' '1') (if (getVar 'checkPrdDl') '==' '1')}}\r\n                            {{assignVar 'checktabTitle' (dashcase (lowercase (sanitize this)))}}\r\n                        {{/or}}\r\n                    {{/if}}\r\n                {{/each}}\r\n                \r\n                <div class=\"oc-description-tabrow\" {{#and (if (getVar 'checkPrdFeatures') '==' '1') (if (getVar 'checktabTitle') '==' 'features-amp-benefits')}}style=\"display: none;\"{{/and}}{{#and (if (getVar 'checkPrdSpec') '==' '1') (if (getVar 'checktabTitle') '==' 'product-specifications')}}style=\"display: none;\"{{/and}}{{#and (if (getVar 'checkPrdDl') '==' '1') (if (getVar 'checktabTitle') '==' 'product-downloads')}}style=\"display: none;\"{{/and}}>\r\n                    <div class=\"tab\">\r\n                        <h2 class=\"tab-title\">\r\n                            {{#each (split item '</h4>')}}{{#if @key '==' '0'}}{{sanitize this}}{{/if}}{{/each}}</h2>\r\n                        <div class=\"icon\">\r\n                            <svg>\r\n                                <use xlink:href=\"#icon-pdp-tab-arrow\" />\r\n                            </svg>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"tab-content oc-productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\r\n                        {{#each (split item '</h4>')}}{{#if @key '==' '1'}}{{{this}}}{{/if}}{{/each}}\r\n                    </div>\r\n                </div>\r\n            {{/eachIndex}}\r\n        {{else}}\r\n            <div class=\"oc-description-tabrow\">\r\n                <div class=\"tab\">\r\n                    <h2 class=\"tab-title\">Product Description</h2>\r\n                    <div class=\"icon\">\r\n                        <svg>\r\n                            <use xlink:href=\"#icon-pdp-tab-arrow\" />\r\n                        </svg>\r\n                    </div>\r\n                </div>\r\n                <div class=\"tab-content oc-productView-description\">\r\n                    {{{product.description}}}\r\n                </div>\r\n            </div>\r\n        {{/contains}}\r\n    {{/if}}\r\n\t\r\n\t{{#if (getVar 'checkPrdSpec') '==' '1'}}\r\n\t\t<div class=\"oc-description-tabrow\">\r\n\t\t\t<div class=\"tab\">\r\n\t\t\t\t<h2 class=\"tab-title\">Product Specifications</h2>\r\n\t\t\t\t<div class=\"icon\">\r\n\t\t\t\t\t<svg>\r\n\t\t\t\t\t\t<use xlink:href=\"#icon-pdp-tab-arrow\" />\r\n\t\t\t\t\t</svg>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"tab-content oc-productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\r\n\t\t\t\t<table class=\"shop_attributes\" style=\"width: 510px;\">\r\n\t\t\t\t\t<tbody>\r\n\t\t\t\t\t\t{{#each product.custom_fields}}\r\n\t\t\t\t\t\t\t{{#startsWith \"_spec_\" name}}\r\n\t\t\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t\t\t<th style=\"width: 169px;\">\r\n\t\t\t\t\t\t\t\t\t\t{{#each (split name '_')}}\r\n\t\t\t\t\t\t\t\t\t\t\t{{#if @last}}\r\n\t\t\t\t\t\t\t\t\t\t\t\t{{this}}\r\n\t\t\t\t\t\t\t\t\t\t\t{{/if}}\r\n\t\t\t\t\t\t\t\t\t\t{{/each}}\r\n\t\t\t\t\t\t\t\t\t</th>\r\n\t\t\t\t\t\t\t\t\t<td style=\"width: 341px;\">{{{value}}}</td>\r\n\t\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t\t{{/startsWith}}\r\n\t\t\t\t\t\t{{/each}}\r\n\t\t\t\t\t</tbody>\r\n\t\t\t\t</table>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t{{/if}}\r\n\t\r\n\t{{#if (getVar 'checkPrdDl') '==' '1'}}\r\n\t\t<div class=\"oc-description-tabrow\">\r\n\t\t\t<div class=\"tab\">\r\n\t\t\t\t<h2 class=\"tab-title\">Product Downloads</h2>\r\n\t\t\t\t<div class=\"icon\">\r\n\t\t\t\t\t<svg>\r\n\t\t\t\t\t\t<use xlink:href=\"#icon-pdp-tab-arrow\" />\r\n\t\t\t\t\t</svg>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t\t<div class=\"tab-content oc-productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\r\n\t\t\t\t<div>\r\n\t\t\t\t    {{#each product.custom_fields}}\r\n\t\t\t\t\t    {{#startsWith \"_dl_\" name}}\r\n\t\t\t\t\t\t    <span style=\"padding-right:8px;\"><a href=\"{{{value}}}\" target=\"_blank\">{{#each (split name '_')}}{{#if @last}}{{this}}{{/if}}{{/each}}</a></span>\r\n\t\t\t\t\t    {{/startsWith}}\r\n\t\t\t\t    {{/each}}\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t{{/if}}\r\n\t\r\n\t<div class=\"oc-description-tabrow\" style=\"display:none;\">\r\n\t\t<div class=\"tab\">\r\n\t\t\t<h2 class=\"tab-title\">Additional Information</h2>\r\n\t\t\t<div class=\"icon\">\r\n\t\t\t\t<svg>\r\n\t\t\t\t\t<use xlink:href=\"#icon-pdp-tab-arrow\" />\r\n\t\t\t\t</svg>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t\t<div class=\"tab-content oc-productView-description\" {{#if settings.data_tag_enabled}} data-event-type=\"product\" {{/if}}>\r\n\t\t\t<table class=\"shop_attributes\" style=\"width: 510px;\">\r\n\t\t\t\t<tbody>\r\n\t\t\t\t\t{{#each product.custom_fields}}\r\n\t\t\t\t\t\t{{#startsWith \"_\" name}}\r\n\t\t\t\t\t\t\t\r\n\t\t\t\t\t\t{{else}}\r\n\t\t\t\t\t\t\t<tr>\r\n\t\t\t\t\t\t\t\t<th style=\"width: 169px;\">\r\n\t\t\t\t\t\t\t\t\t{{name}}\r\n\t\t\t\t\t\t\t\t</th>\r\n\t\t\t\t\t\t\t\t<td style=\"width: 341px;\">{{{value}}}</td>\r\n\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t{{/startsWith}}\r\n\t\t\t\t\t{{/each}}\r\n\t\t\t\t</tbody>\r\n\t\t\t</table>\r\n\t\t</div>\r\n\t</div>\r\n    \r\n    {{!-- {{#if theme_settings.show_custom_fields_tabs '!==' true}}\r\n        {{#if product.custom_fields}}\r\n        <div class=\"oc-description-tabrow\">\r\n            <div class=\"tab\">\r\n                <h2 class=\"tab-title\">Additional Information</h2>\r\n                \r\n            </div>\r\n            <div class=\"oc-productView-description oc-customfield\">\r\n                \r\n                <div class=\"custom-row\">\r\n                {{> components/1center-custom/oc-custom-fields }}\r\n                </div>\r\n                \r\n            </div>\r\n        </div>\r\n        {{/if}}\r\n    {{/if}} --}}\r\n    \r\n    {{#if product.videos.list.length}}\r\n        <div class=\"oc-description-tabrow\">\r\n            <div class=\"tab\">\r\n                <h2 class=\"tab-title\"> {{lang 'products.videos.header'}}</h2>\r\n            </div>\r\n            <div class=\"oc-productView-description\">\r\n                {{> components/1center-custom/oc-videos product.videos}}\r\n            </div>\r\n        </div>\r\n        {{/if}}\r\n    {{#if product.warranty}}\r\n    <div class=\"oc-description-tabrow\">\r\n        <div class=\"tab\">\r\n            <h2 class=\"tab-title\">{{lang 'products.warranty'}}</h2>\r\n        </div>\r\n        <div class=\"oc-productView-description\">\r\n            {{{product.warranty}}}\r\n        </div>\r\n        </div>\r\n    {{/if}}\r\n\r\n</div>\r\n","components/1center-custom/oc-videos":"<section class=\"toggle videoGallery\" data-video-gallery>\r\n    \r\n\r\n    <div class=\"toggle-content is-open\" id=\"videoGallery-content\">\r\n        <div class=\"videoGallery-main\">\r\n            <iframe\r\n                id=\"player\"\r\n                class=\"lazyload\"\r\n                type=\"text/html\"\r\n                width=\"640\"\r\n                height=\"390\"\r\n                title=\"videoframe\"\r\n                frameborder=\"0\"\r\n                webkitAllowFullScreen\r\n                mozallowfullscreen\r\n                allowFullScreen\r\n                data-src=\"//www.youtube.com/embed/{{this.featured.id}}?rel=0\"\r\n                data-video-player>\r\n            </iframe>\r\n        </div>\r\n        <ul class=\"videoGallery-list\">\r\n        {{#each this.list}}\r\n            <li class=\"videoGallery-item\">\r\n                <a href=\"#\" class=\"video {{#if @first}}is-active{{/if}}\" data-video-item data-video-id=\"{{id}}\">\r\n                    <div class=\"video-figure\">\r\n                        <img class=\"lazyload\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"//i.ytimg.com/vi/{{id}}/default.jpg\"/>\r\n                    </div>\r\n                    <div class=\"video-body\">\r\n                        <h5 class=\"video-title\">{{title_long}}</h5>\r\n                        <p class=\"video-description\">{{description_short}}</p>\r\n                    </div>\r\n                </a>\r\n            </li>\r\n        {{/each}}\r\n        </ul>\r\n    </div>\r\n\r\n</section>\r\n"}
